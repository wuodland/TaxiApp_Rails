<h3>Search Result</h3>  

    <% @results.each do |result| %>  

	     <table>
  			<thead>
    			<tr>
      				<th>Quote</th>
      				<th>Author</th>
      				<th>Category</th>
			      <th colspan="3"></th>
			    </tr>
			  </thead>
						
			  <tbody>
			      <tr>
			        <td><%= result.quote %></td>
			        <td><%= result.author_name %></td>
			        <td><%= result.category %></td>
     			</tr>
  			</tbody>
		</table>

    <% end %>